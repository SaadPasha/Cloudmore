{
  "api_host_config": {
    "api_web_protocol": "https",
    "api_web_host": "petstore.swagger.io",
    "api_ver": "v2"
  },
  "headers": {
    "Content-Type": "application/json"
  },
  "api_endpoints": {
    "user": {
      "create_single_user": "/user",
      "create_multiple_users": "/user/createWithList",
      "get_user_details": "/user/{}",
      "update_user_details": "/user/{}",
      "delete_user": "/user/{}"
    }
  },
  "urls": {
    "home_page": "https://cloudmore.com",
    "platform_page": "https://cloudmore.com/product/",
    "blog_page": "https://cloudmore.com/content-hub",
    "case_studies_page": "https://cloudmore.com/content-hub",
    "about_page": "https://cloudmore.com/about-us/",
    "contact_page": "https://cloudmore.com/contact-us/"
  },
  "log_settings": {
    "log_level": "debug",
    "log_dir_name": "api-tests-logs",
    "file_write_mode": true
  }
}